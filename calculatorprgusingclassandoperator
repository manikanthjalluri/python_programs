class calc:
    def add(self,x,y):
        return x+y
    def subtract(self,x,y):
        return x-y
    def multiply(self,x,y):
        return x*y
    def divide(self,x,y):
        if y!=0:
            return x/y
        else:
            return("cannot divide by zero")
z=calc()
print(z.add(7,5))
print(z.subtract(7,5))
print(z.multiply(7,5))
print(z.divide(7,5))
